package com.abhi;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.context.ConfigurableApplicationContext;

import com.abhi.dao.StudentRepo;
import com.abhi.entities.User;

@SpringBootApplication
public class SpringJpaApplication {

	public static void main(String[] args) {
		
		ConfigurableApplicationContext context = SpringApplication.run(SpringJpaApplication.class, args);
		StudentRepo repo = context.getBean(StudentRepo.class);
		User user = new User();
		//user.setId(10);
		user.setName("Akhilesh");
		user.setAddress("Naini");
		
		
		User user1 = repo.save(user);
		System.out.println("uswer1 "+user1);
	}

}
